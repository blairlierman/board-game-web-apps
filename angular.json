{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "projects": {
    "farming-game-remote-host": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "apps/farming-game-remote-host",
      "sourceRoot": "apps/farming-game-remote-host/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/farming-game-remote-host",
            "index": "apps/farming-game-remote-host/src/index.html",
            "main": "apps/farming-game-remote-host/src/main.ts",
            "polyfills": "apps/farming-game-remote-host/src/polyfills.ts",
            "tsConfig": "apps/farming-game-remote-host/tsconfig.app.json",
            "assets": [
              "apps/farming-game-remote-host/src/favicon.ico",
              "apps/farming-game-remote-host/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/purple-green.css",
              "apps/farming-game-remote-host/src/styles.scss"
            ],
            "scripts": [],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/farming-game-remote-host/src/environments/environment.ts",
                  "with": "apps/farming-game-remote-host/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ]
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "farming-game-remote-host:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "farming-game-remote-host:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "farming-game-remote-host:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/farming-game-remote-host/src/test.ts",
            "polyfills": "apps/farming-game-remote-host/src/polyfills.ts",
            "tsConfig": "apps/farming-game-remote-host/tsconfig.spec.json",
            "karmaConfig": "apps/farming-game-remote-host/karma.conf.js",
            "assets": [
              "apps/farming-game-remote-host/src/favicon.ico",
              "apps/farming-game-remote-host/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/purple-green.css",
              "apps/farming-game-remote-host/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "deploy": {
          "builder": "@angular/fire:deploy",
          "options": {}
        }
      }
    },
    "farming-game-remote-host-e2e": {
      "root": "apps/farming-game-remote-host-e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "apps/farming-game-remote-host-e2e/protractor.conf.js",
            "devServerTarget": "farming-game-remote-host:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "farming-game-remote-host:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "apps/farming-game-remote-host-e2e/tsconfig.json"
          }
        }
      },
      "implicitDependencies": [
        "farming-game-remote-host"
      ],
      "tags": []
    },
    "sff-tracker": {
      "projectType": "application",
      "root": "apps/sff-tracker",
      "sourceRoot": "apps/sff-tracker/src",
      "prefix": "farming-game-remote-host",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/sff-tracker",
            "index": "apps/sff-tracker/src/index.html",
            "main": "apps/sff-tracker/src/main.ts",
            "polyfills": "apps/sff-tracker/src/polyfills.ts",
            "tsConfig": "apps/sff-tracker/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/sff-tracker/src/favicon.ico",
              "apps/sff-tracker/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/purple-green.css",
              "apps/sff-tracker/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/sff-tracker/src/environments/environment.ts",
                  "with": "apps/sff-tracker/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "sff-tracker:build:production"
            },
            "development": {
              "browserTarget": "sff-tracker:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "sff-tracker:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/sff-tracker/**/*.ts",
              "apps/sff-tracker/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/sff-tracker"
          ],
          "options": {
            "jestConfig": "apps/sff-tracker/jest.config.ts",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "sff-tracker-e2e": {
      "root": "apps/sff-tracker-e2e",
      "sourceRoot": "apps/sff-tracker-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/sff-tracker-e2e/cypress.config.ts",
            "devServerTarget": "sff-tracker:serve:development",
            "testingType": "e2e"
          },
          "configurations": {
            "production": {
              "devServerTarget": "sff-tracker:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/sff-tracker-e2e/**/*.{js,ts}"
            ]
          }
        }
      },
      "tags": [],
      "implicitDependencies": [
        "sff-tracker"
      ]
    }
  }
}
